	<script language="javascript">

		function AlanKontrolveKayit() {
			var UyariMesaji = "";
			var frm;
			frm = document.getElementById("FRM_SKT02009");

			if (document.getElementById("lblTcKimlikNo").innerHTML == "") {
				UyariMesaji = UyariMesaji + "Lütfen Kayıt Yapmak İstediğiniz Adayı Seçiniz.\n";
			}

			if (frm.cmbIstenenEhliyetSinifi.value == "-1" || frm.cmbIstenenEhliyetSinifi.value == "") {
				UyariMesaji = UyariMesaji + "İstenen Ehliyet Sınıfı Seçilmiş Olmalıdır.\n";
			}


			if (frm.cmbMevcutEhliyetSinifi.value != "-1") {
				UyariMesaji = UyariMesaji + TextAlan(frm.txtEhliyetBelgeNo.value, "Mevcut Ehliyet Belge No", 10, false);
			}

			if (frm.cmbIstenenEhliyetSinifi.value == frm.cmbMevcutEhliyetSinifi.value) {
				UyariMesaji = UyariMesaji + "Mevcut Sertifika Sınıfı ve İstenen Sertifika Sınıfı Aynı Olamaz.\n";
			}
			UyariMesaji = UyariMesaji + TarihAlan(frm.Us_tarih1_txtTarihGiris.value, "Dönem Bitiş Tarihi", false);
			//UyariMesaji=UyariMesaji+NumAlan(frm.Gv_kod1_txtGuvenlikKod.value,"Güvenlik Kodu",9999,0,false);

			if (UyariMesaji != "") {
				alert(UyariMesaji);
				return false;
			}
			else {
				if (Beklet(13)) {
					frm.hiddenKaydet.value = "Kaydet";
					frm.submit();
					return true;
				}
			}
		}

		function fnIslemSec(KayitNo) {
			var frm;
			var UyariMesaji = "";
			frm = document.getElementById("FRM_SKT02009");

			//UyariMesaji=UyariMesaji+NumAlan(frm.Gv_kod1_txtGuvenlikKod.value,"Güvenlik Kodu",9999,0,false);
			if (UyariMesaji != "") {
				alert(UyariMesaji);
				return false;
			}
			else {
				if (Beklet(13)) {
					frm.hdnKayitSec.value = KayitNo;
					frm.submit();
					return true;
				}
			}
		}

		function fnIslemSecSilme(KayitNo) {
			var frm;
			var onay;
			var UyariMesaji = "";
			frm = document.getElementById("FRM_SKT02009");

			//UyariMesaji=UyariMesaji+NumAlan(frm.Gv_kod1_txtGuvenlikKod.value,"Güvenlik Kodu",9999,0,false);
			if (UyariMesaji != "") {
				alert(UyariMesaji);
				return false;
			}
			else {
				onay = confirm("Seçmiş Olduğunuz Dönem Kaydı Silinecek İşlemi Onaylıyor musunuz ?");
				if (onay) {
					if (Beklet(13)) {
						frm.hdnKayitNoSilmeIcin.value = KayitNo;
						frm.submit();
						return true;
					}
				}
				else
					return false;
			}
		}
	</script>
