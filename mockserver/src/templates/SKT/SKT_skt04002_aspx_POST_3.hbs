		function AlanKontrolveKayit()
		{
			var UyariMesaji = "";
			var frm;
			frm = document.getElementById("FRM_SKT04002");
			
			if (frm.cmbDonemi.value=="-1")
			{
				UyariMesaji=UyariMesaji+"Dönemi Boş Geçilemez.\n";
			}
			
			if (frm.cmbGrupAdi.value=="-1")
			{
				UyariMesaji=UyariMesaji+"Grup Adı Boş Geçilemez.\n";
			}
		
			UyariMesaji=UyariMesaji+TarihAlan(frm.Us_tarih1_txtTarihGiris.value ,"Dönem Başlangıç Tarihi",false);
			
			if (frm.cmbKurumOnayDurumu.value=="-1")
			{
				UyariMesaji=UyariMesaji+"Kurum Onay Durumu Boş Geçilemez.\n";
			}
			
			////////////UyariMesaji=UyariMesaji+NumAlan(frm.Gv_kod1_txtGuvenlikKod.value,"Güvenlik Kodu",9999,0,false);
		
			
			if (UyariMesaji != "")
			{
				alert(UyariMesaji);
				return false;
			}
			else
			{ 
				if (Beklet(13))
				{
					frm.hiddenKaydet.value= "Kaydet";
					frm.submit();
					return true;
				}
			}
			
		}
		
		
		
		function fnIslemSecSilme (KayitNo)
		{
			var frm;
			frm = document.getElementById("FRM_SKT04002");
			
			var onay = confirm(" Seçmiş Olduğunuz Döneme Ait Grup ve Şube Tanımlamalarınız ile ilgili grup ve şubeye tanımlanmış Teorik Dersler, Direksiyon Eğitim Dersleri ve Aday Bilgileri Silinecek, İlgili Kayıtı Silmek İstediğinize Emin misiniz?");    

			/* Question icon, Yes/No button. */
			/* 0 = error, 1 = OK, 2 = Cancel, 3 = No, 4 = Yes */

			if (onay)
			{
				if (Beklet(13))
				{
					frm.hdnKayitNoSilmeIcin.value= KayitNo;
					frm.submit();
					return true;
				}
			}
			else
			{
				return false;
			}
		}	
		
		function fnIslemSec(KayitNo)
		{
			var frm;
			frm = document.getElementById("FRM_SKT04002");
			if (Beklet(13))
			{
				frm.hdnKayitSec.value= KayitNo;
				frm.submit();
				return true;
			}
		}
		
		
		
		function YeniKayit()
		{
			var frm;
			frm = document.getElementById("frm_SKT04002");
			
			if (Beklet(13))
			{
				frm.pageMode.value="yeniKayit";
				frm.bosalt.value="BOSALT";
				frm.submit();
				return true;
			}
		}
		
